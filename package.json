{
  "name": "magic-organizer",
  "version": "1.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ssl-server": "http-server -p 8080 -c-1 dist/",
    "i18n": "ng xi18n --output-path=src/app/i18n --i18nLocale=en",
    "extract-i18n": "ng xi18n magic-organizer --i18n-format xlf --output-path src/app/i18n --i18n-locale en && ng run magic-organizer:xliffmerge",
    "start-es": "ng serve --configuration=es",
    "start-de": "ng serve --configuration=de",
    "start-pt": "ng serve --configuration=pt",
    "start-fr": "ng serve --configuration=fr",
    "start-ru": "ng serve --configuration=ru",
    "build-i18n": "./build-i18n.sh",
    "i18n-ssl-server": "npm run build-i18n && http-server -p 8080 -c-1 dist/magic-organizer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/pwa": "^0.803.19",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "@ngxs-labs/select-snapshot": "^1.1.0",
    "@ngxs/schematics": "0.0.1-alpha.5",
    "@ngxs/storage-plugin": "^3.5.1",
    "@ngxs/store": "^3.5.1",
    "@types/underscore": "^1.9.3",
    "angular-calendar": "^0.27.21",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.6.10",
    "date-fns": "^1.30.1",
    "google-palette": "^1.1.0",
    "ngx-material-timepicker": "^5.2.2",
    "observable-webworker": "^3.2.3",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
